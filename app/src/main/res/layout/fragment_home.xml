<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".HomeFragment">

    <ImageView
        android:id="@+id/homematching_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/homematching_btn"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="40dp"
        android:paddingBottom="40dp"/>

    <ImageView
        android:id="@+id/homecarfool_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/homecarfool_btn"
        app:layout_constraintTop_toTopOf="@id/homematching_btn"
        app:layout_constraintStart_toEndOf="@id/homematching_btn"
        android:layout_marginLeft="8dp" />

</androidx.constraintlayout.widget.ConstraintLayout>